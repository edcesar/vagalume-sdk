{
    "name": "giorgiolucca/vagalume-sdk",
    "homepage": "https://github.com/giorgiolucca/vagalume-sdk",
    "description": "SDK nÃ£o oficial da API do vagalume desenvolvida em PHP (https://api.vagalume.com.br/)",
    "keywords": ["vagalume","sdk","api"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Giorgio Lucca",
            "email": "giorgiolucca@hotmail.com",
            "homepage": "https://github.com/giorgiolucca"
        }
    ],
    "support": {
        "issues": "https://github.com/giorgiolucca/vagalume-sdk/issues"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~5.3|~6.0.1|~6.1",
        "guzzlehttp/psr7": "~1.0",
        "guzzlehttp/promises": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "GiorgioLucca\\VagalumeSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GiorgioLucca\\VagalumeSdk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}